            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor">
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="script">import java.util.regex.*;
String res = prev.getResponseDataAsString();
Matcher m = Pattern.compile(&quot;&lt;option value=\&quot;([^\&quot;]+)\&quot;&gt;&quot;).matcher(res);
String[] city = new String[14];
int i = 0;
while (m.find() &amp;&amp; i &lt; 14) {
    city[i++] = m.group(1);
}
if (i == 14) {
    vars.put(&quot;from&quot;,city[1+(int)(Math.random()*7)]);
    vars.put(&quot;to&quot;,city[7 + (int)(Math.random()*7)]);
    log.info(&quot;From = &quot; + vars.get(&quot;from&quot;) + &quot;, To = &quot; + vars.get(&quot;to&quot;));
} else {
    log.error(&quot;Expected 14 cities but found &quot; + i);
}
</stringProp>

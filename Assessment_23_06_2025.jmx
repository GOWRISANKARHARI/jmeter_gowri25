            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">import java.util.regex.*;
String res = prev.getResponseDataAsString();
Pattern price = Pattern.compile(&quot;Price:\\s*(\\d+(\\.\\d+)?)&quot;);
Matcher pricechecker = price.matcher(res);
double price = 0;
if (pricechecker.find()) 
{
    price = Double.parseDouble(pricechecker.group(1));
}
Pattern fee = Pattern.compile(&quot;Taxes: (\\d+(\\.\\d+)?)&lt;/p&gt;&quot;);
Matcher feechecker = fee.matcher(res);
double fee = 0;
if(feechecker.find())
{
	fee = Double.parseDouble(feechecker.group(1));
}
Double tot = price + fee ;
Pattern pat = Pattern.compile(&quot;&lt;em&gt;(\\d+(\\.\\d+)?)&lt;/em&gt;&quot;);
Matcher check = pat.matcher(res);
if (check.find()) 
{
    String Total_amount = check.group(1).trim();
    log.info(&quot;Total amount: &quot; + Total_amount);
    if (Double.parseDouble(Total_amount) == tot) 
    {
        log.info(&quot;The total amount is equal.&quot;);
    }
    else 
    {
        log.warn(&quot;The total amount is NOT equal.&quot;);
    }
}
else
{
log.error(&quot;The value of the total amount is NOT found.&quot;);
}
</stringProp>

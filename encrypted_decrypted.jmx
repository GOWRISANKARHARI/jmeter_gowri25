        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler enc password">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import javax.crypto.Cipher
import javax.crypto.spec.SecretKeySpec
import java.util.Base64

// Secret key (16 bytes for AES-128)
String secretKey = &quot;1234567890123456&quot;

// Create key and cipher
SecretKeySpec key = new SecretKeySpec(secretKey.getBytes(&quot;UTF-8&quot;), &quot;AES&quot;)
Cipher cipher = Cipher.getInstance(&quot;AES&quot;)

// Encrypt the password
String plainText = &quot;Password123&quot;
cipher.init(Cipher.ENCRYPT_MODE, key)
byte[] encryptedBytes = cipher.doFinal(plainText.getBytes(&quot;UTF-8&quot;))
String encryptedPassword = Base64.getEncoder().encodeToString(encryptedBytes)

log.info(&quot;Encrypted Password: &quot; + encryptedPassword)

// Save encrypted password to a variable
vars.put(&quot;encryptedPassword&quot;, encryptedPassword)
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
        <hashTree/>
        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler decrypt" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import javax.crypto.Cipher
import javax.crypto.spec.SecretKeySpec
import java.util.Base64

String secretKey = &quot;1234567890123456&quot;
SecretKeySpec key = new SecretKeySpec(secretKey.getBytes(&quot;UTF-8&quot;), &quot;AES&quot;)
Cipher cipher = Cipher.getInstance(&quot;AES&quot;)

String encryptedText = vars.get(&quot;encryptedPassword&quot;)
cipher.init(Cipher.DECRYPT_MODE, key)
byte[] decryptedBytes = cipher.doFinal(Base64.getDecoder().decode(encryptedText))
String decryptedText = new String(decryptedBytes, &quot;UTF-8&quot;)

log.info(&quot;Decrypted Password: &quot; + decryptedText)
vars.put(&quot;decryptedPassword&quot;, decryptedText)
</stringProp>
        </JSR223Sampler>
